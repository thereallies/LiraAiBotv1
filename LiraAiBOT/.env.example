# === ОСНОВНЫЕ API КЛЮЧИ ===
# OpenRouter API ключи (можно несколько)
OPENROUTER_API_KEY=sk-or-v1-xxx
# OPENROUTER_API_KEY1=sk-or-v1-yyy
# OPENROUTER_API_KEY2=sk-or-v1-zzz

# === TELEGRAM ===
TELEGRAM_BOT_TOKEN=your_telegram_bot_token

# === GROQ (быстрые бесплатные модели) ===
GROQ_API_KEY=gsk_xxx
GROQ_BASE_URL=https://api.groq.com/openai/v1

# === CEREBRAS (сверхбыстрые модели) ===
CEREBRAS_API_KEY=csk_xxx

# === ГЕНЕРАЦИЯ ИЗОБРАЖЕНИЙ ===
HF_API_KEY=your_huggingface_api_key

# === ГОЛОСОВЫЕ ФУНКЦИИ (опционально) ===
# ElevenLabs для TTS (высокое качество)
ELEVEN_API_KEY=your_elevenlabs_api_key
ELEVEN_VOICE_ID=your_elevenlabs_voice_id
ELEVEN_MODEL_ID=eleven_multilingual_v2

# === ЭМБЕДДИНГИ ===
EMBEDDING_API_KEY=your_openai_embedding_key

# === НАСТРОЙКИ БЕЗОПАСНОСТИ ===
# ID администратора бота
ADMIN_USER_ID=

# CORS источники (через запятую), * - разрешить все
CORS_ORIGINS=*

# DEBUG режим (true/false)
DEBUG=false
